<div class="main">
    <div class="row">
    	<div class="col-md-4"><img class="img-responsive" src='<%= game.imageUrl %>'></div>
    	<div class="col-md-8">
    		<h1><%= game.name %></h1>
    		<p><%= game.description %></p>
    	</div>
	</div>

    <% for( var i = 0; i < gameInstances.length; i++ ) {%>
    	<div class="row">
	    		<div class="col-md-4">Game ID</div>
	    		<div class="col-md-4">Game Name</div>
	    		<div class="col-md-4">Players</div>
    	</div>
        <div class="row">
            <div class="col-md-4"><%= gameInstances[i].gameId %></div>
            <div class="col-md-4"><%= gameInstances[i].name %></div>
            <div class="col-md-4">
            <% for( var j = 0; j < gameInstances[i].playerNames.length; j++) {%>
            	<div class="col-md-2"><%= gameInstances[i].playerNames[j] %></div>
            <% } %>
            </div>
        </div>
    <% } %>
</div>
